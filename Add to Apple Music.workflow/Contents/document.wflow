					<string># Get the workflow's Contents directory
# In Automator, we need to find where this workflow is located
WORKFLOW_PATH="${0%/Contents/document.wflow}"
if [[ "$WORKFLOW_PATH" == "$0" ]]; then
    # Fallback: try to find the workflow in common locations
    WORKFLOW_PATH="$HOME/Library/Services/Add to Apple Music.workflow"
    if [ ! -d "$WORKFLOW_PATH" ]; then
        WORKFLOW_PATH="/Users/evan/Developer/Repositories/@github.com/evan-kinney/macos-extensions/Add to Apple Music.workflow"
    fi
fi

CONTENTS_DIR="$WORKFLOW_PATH/Contents"
VENV_DIR="$HOME/.local/venvs/add-to-apple-music"
VENV_PYTHON="$VENV_DIR/bin/python"
REQUIREMENTS_FILE="$WORKFLOW_PATH/requirements.txt"

# Create venv if it doesn't exist
if [ ! -f "$VENV_PYTHON" ]; then
    echo "Creating virtual environment at $VENV_DIR..."
    mkdir -p "$(dirname "$VENV_DIR")"
    python3 -m venv "$VENV_DIR"
    
    # Install dependencies
    echo "Installing required packages..."
    "$VENV_PYTHON" -m pip install --upgrade pip
    "$VENV_PYTHON" -m pip install pyacoustid musicbrainzngs mutagen tk
    echo "Virtual environment setup complete!"
fi

# Run the script with the venv Python
"$VENV_PYTHON" "$CONTENTS_DIR/add_to_music.py" "$@"</string>
